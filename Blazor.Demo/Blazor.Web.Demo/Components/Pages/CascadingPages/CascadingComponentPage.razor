@page "/cascading-component"
@rendermode RenderMode.InteractiveServer
@using Models

<h3>Cascading Component Value</h3>
<div>@Foo.Name-@Foo.Id</div>

<p></p>
<p>Cascading Value:</p>
<CascadingValue Value="@Foo">
    <CascadingChild></CascadingChild>
</CascadingValue>

@code {
    private Foo Foo = new() { Name = "Cascading Parent Foo" };

    protected override void OnInitialized()
    {
        Console.WriteLine($"OnInitialized {Foo.Name}:{Foo.Id}");
    }
}
